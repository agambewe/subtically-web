v-spacer>\n                        <v-btn-toggle v-model=\"sortDesc\" mandatory>\n                            <v-btn large depressed color=\"#4E5862\" :value=\"false\">\n                                <v-icon>mdi-arrow-up</v-icon>\n                            </v-btn>\n                            <v-btn large depressed color=\"#4E5862\" :value=\"true\">\n                                <v-icon>mdi-arrow-down</v-icon>\n                            </v-btn>\n                        </v-btn-toggle>\n                    </template>\n                </v-toolbar>\n            </template>\n\n            <template v-slot:default=\"props\">\n                <v-row>\n                    <v-col v-for=\"item in props.items\" :key=\"item.title\" cols=\"12\" sm=\"12\">\n                        <!-- <v-card> -->\n                            <v-card color=\"white\" class=\"black--text\">\n                            <v-layout row>\n                                <v-flex xs3>\n                                <v-img\n                                    src=\"https://cdn.vuetifyjs.com/images/cards/halcyon.png\"\n                                    height=\"225px\"\n                                    contain\n                                ></v-img>\n                                </v-flex>\n                                <v-flex xs9>\n                                    <v-card-title primary-title>\n                                        <div>\n                                            <div class=\"headline\">{{ item.title }}</div>\n                                            <span class=\"grey--text\">{{item.detail_user.name}} - </span>\n                                            <div>{{ item.description }}</div>\n                                            <v-btn text color=\"blue lighten-2\">Read More</v-btn>  \n                                        </div>\n                                    </v-card-title>\n                                </v-flex>\n                            </v-layout>\n                            <v-divider light></v-divider>\n                            </v-card>\n                    </v-col>\n                </v-row>\n            </template>\n\n            <template v-slot:footer>\n                <v-row class=\"mt-2\" align=\"center\" justify=\"center\">\n                    <v-spacer></v-spacer>\n                    <span class=\"mr-4 grey--text\">Page {{ page }} of {{ numberOfPages }}</span>\n                    <v-btn fab dark color=\"blue darken-3\" class=\"mr-1\" @click=\"formerPage\">\n                    prev\n                    </v-btn>\n                    <v-btn fab dark color=\"blue darken-3\" class=\"ml-1\" @click=\"nextPage\">\n                    next\n                    </v-btn>\n                </v-row>\n            </template>\n        </v-data-iterator>\n    </v-container>\n</v-container>\n</template>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css?family=Share+Tech+Mono\");\n\ntable th+th {\n    border-left: 1px solid #dddddd;\n}\n\ntable td+td {\n    border-left: 1px solid #dddddd;\n}\n\ntbody tr:nth-of-type(odd) {\n    background-color: rgba(0, 0, 0, .05);\n}\n\n.v-data-table\n    /deep/\n    tbody\n    /deep/\n    tr:hover:not(.v-data-table__expanded__content) {\n        background: #8797a8 !important;\n    }\n    \n.flex {\n    display: -webkit-box;\n    display: -moz-box;\n    display: -ms-flexbox;\n    display: -webkit-flex;\n    display: flex;\n}\n\n.black--text:hover{\n    color: red;\n}\n</style>\n\n<script>\nexport default {\n    data() {\n        return {\n            load: false,\n            videos: [],\n            filter: {},\n            sortDesc: false,\n            page: 1,\n            itemsPerPage: 2,\n            sortBy: 'title',\n            checkDelete: [],\n            search: \"\",\n            editedIndex: -1,\n            selectedCard: -1\n        }\n    },\n    computed: {\n        numberOfPages() {\n            return Math.ceil(this.videos.length / this.itemsPerPage)\n        },\n    },\n    watch: {\n    },\n    methods: {\n        nextPage() {\n            if (this.page + 1 <= this.numberOfPages) this.page += 1\n        },\n        formerPage() {\n            if (this.page - 1 >= 1) this.page -= 1\n        },\n        updateItemsPerPage(number) {\n            this.itemsPerPage = number\n        },\n        readData() {\n            var uri = this.$apiUrl + '/videos'\n            this.$http.get(uri).then(response => {\n                this.videos = response.data\n                this.typeHeader='Daftar Produk Tersedia'\n            })\n        },\n    },\n    mounted() {\n        this.readData();\n    },\n}\n</script>\n"]}]}